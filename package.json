{
  "name": "prohelper_land",
  "version": "1.0.0",
  "description": "ProHelper - умная платформа для управления строительными проектами. Автоматизация учета материалов, контроль бюджета и координация команд.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npx tsc && vite build && vite-plugin-ssr build && vite-plugin-ssr prerender",
    "build:lk": "cross-env BUILD_TARGET=lk npx tsc && vite build --outDir dist-lk",
    "ssr:dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite-plugin-ssr preview"
  },
  "keywords": [
    "ProHelper",
    "строительство",
    "управление проектами",
    "учет материалов",
    "строительная отчетность",
    "SaaS для строителей",
    "автоматизация строительства",
    "финансовый контроль",
    "прораб",
    "строительные технологии"
  ],
  "author": "ProHelper Team",
  "license": "ISC",
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@unhead/dom": "^2.0.11",
    "@unhead/react": "^2.0.11",
    "axios": "^1.9.0",
    "framer-motion": "^12.9.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^11.0.5",
    "typescript": "^5.8.3",
    "chart.js": "^4.4.1",
    "react-chartjs-2": "^5.3.0",
    "vite-plugin-ssr": "^0.4.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "^22.15.3",
    "@types/react-toastify": "^4.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4",
    "cross-env": "^7.0.3"
  }
}

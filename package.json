{
  "name": "prohelper_land",
  "version": "1.0.0",
  "description": "ProHelper - умная платформа для управления строительными проектами. Автоматизация учета материалов, контроль бюджета и координация команд.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env ROLLUP_NO_NATIVE=1 npx tsc && npx vite build && npm run copy-manifest",
    "build:marketing": "cross-env ROLLUP_NO_NATIVE=1 BUILD_TARGET=marketing vite build && mkdir -p dist/server && cp server/index.cjs dist/server/ && npm run copy-manifest",
    "build:lk": "cross-env ROLLUP_NO_NATIVE=1 BUILD_TARGET=lk npx tsc && cross-env ROLLUP_NO_NATIVE=1 BUILD_TARGET=lk npx vite build --outDir dist-lk && npm run copy-manifest:lk",
    "copy-manifest": "node scripts/copyManifest.js dist/client",
    "copy-manifest:lk": "node scripts/copyManifest.js dist-lk/client",
    "ssr:dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite-plugin-ssr preview"
  },
  "keywords": [
    "ProHelper",
    "строительство",
    "управление проектами",
    "учет материалов",
    "строительная отчетность",
    "SaaS для строителей",
    "автоматизация строительства",
    "финансовый контроль",
    "прораб",
    "строительные технологии"
  ],
  "author": "ProHelper Team",
  "license": "ISC",
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@unhead/dom": "^2.0.11",
    "@unhead/react": "^2.0.11",
    "axios": "^1.9.0",
    "framer-motion": "^12.9.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.23.0",
    "react-router": "^6.23.0",
    "react-toastify": "^11.0.5",
    "typescript": "^5.8.3",
    "chart.js": "^4.4.1",
    "react-chartjs-2": "^5.3.0",
    "vite-plugin-ssr": "^0.4.142"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "^22.15.3",
    "@types/react-toastify": "^4.0.2",
    "@vitejs/plugin-react": "^4.4.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.3.1",
    "esbuild": "^0.18.15",
    "cross-env": "^7.0.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.40.1",
    "@esbuild/linux-x64": "^0.18.15"
  }
}
